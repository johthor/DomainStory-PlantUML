@startuml
'!include <DomainStory/domainStory>
!include ../domainStory.puml
'!include https://raw.githubusercontent.com/johthor/DomainStory-PlantUML/main/domainStory.puml

Boundary(actors) {
    introduce(person, Person(Person Label))
    introduce(group, Group(Group Label))
    introduce(system, System(System Label))
}

Boundary(object, work objects) {
    introduce(document, Document(Document Label))
    introduce(folder, Folder(Folder Label))
    introduce(call, Call(Call Label))
    introduce(email, Email(Email Label))
    introduce(conversation, Conversation(Conversation Label))
    introduce(info, Info(Info Label))
}
@enduml
